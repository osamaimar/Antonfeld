{
  "version": "0.4",
  "title": "Find Our Office Section",
  "type": "section",
  "content": [
    {
      "id": "find-office-section",
      "elType": "section",
      "isInner": false,
      "settings": {
        "layout": "boxed",
        "content_width": {
          "unit": "px",
          "size": 1200
        },
        "gap": "default",
        "background_background": "classic",
        "background_color": "#F9FAFB",
        "border_radius": {
          "unit": "px",
          "size": 16
        },
        "padding": {
          "unit": "px",
          "top": "32",
          "right": "32",
          "bottom": "32",
          "left": "32",
          "isLinked": false
        }
      },
      "elements": [
        {
          "id": "find-office-inner",
          "elType": "section",
          "isInner": true,
          "settings": {
            "layout": "boxed",
            "gap": "default"
          },
          "elements": [
            {
              "id": "info-column",
              "elType": "column",
              "isInner": true,
              "settings": {
                "_column_size": 50,
                "_inline_size": null,
                "content_position": "middle"
              },
              "elements": [
                {
                  "id": "heading",
                  "elType": "widget",
                  "widgetType": "heading",
                  "settings": {
                    "title": "<span style=\"color: #0EA5E9;\">Find Our</span> <span style=\"color: #0F766E;\">Office</span>",
                    "size": "h3",
                    "typography_typography": "custom",
                    "typography_font_size": { "unit": "px", "size": 24 },
                    "typography_font_weight": "700",
                    "color": "#111827",
                    "margin": { "unit": "px", "top": "0", "right": "0", "bottom": "16", "left": "0", "isLinked": false }
                  }
                },
                {
                  "id": "description",
                  "elType": "widget",
                  "widgetType": "text-editor",
                  "settings": {
                    "editor": "<p>Located in the heart of <span style=\"color: #0EA5E9; font-weight: 600;\">Healthcare City</span>, our <span style=\"color: #0F766E; font-weight: 600;\">modern medical facility</span> offers <span style=\"color: #0EA5E9; font-weight: 600;\">convenient parking</span> and easy access via public transportation.</p>",
                    "text_color": "#6B7280",
                    "typography_typography": "custom",
                    "typography_font_size": { "unit": "px", "size": 16 },
                    "margin": { "unit": "px", "top": "0", "right": "0", "bottom": "24", "left": "0", "isLinked": false }
                  }
                },
                {
                  "id": "highway",
                  "elType": "widget",
                  "widgetType": "icon-list",
                  "settings": {
                    "icon_list": [
                      {
                        "text": "Easy highway access from I-95 and Route 1",
                        "selected_icon": { "value": "fas fa-route", "library": "fa-solid" }
                      }
                    ],
                    "icon_color": "#0EA5E9",
                    "text_color": "#374151",
                    "typography_typography": "custom",
                    "typography_font_size": { "unit": "px", "size": 16 },
                    "space_between": { "unit": "px", "size": 8 }
                  }
                }
              ]
            },
            {
              "id": "map-card-column",
              "elType": "column",
              "isInner": true,
              "settings": {
                "_column_size": 50,
                "_inline_size": null,
                "background_background": "classic",
                "background_color": "#FFFFFF",
                "border_radius": { "unit": "px", "size": 12 },
                "box_shadow_box_shadow_type": "yes",
                "box_shadow_box_shadow": { "horizontal": 0, "vertical": 6, "blur": 16, "spread": -4, "color": "rgba(0,0,0,0.12)" },
                "padding": { "unit": "px", "top": "24", "right": "24", "bottom": "24", "left": "24", "isLinked": false }
              },
              "elements": [
                {
                  "id": "map-image",
                  "elType": "widget",
                  "widgetType": "image",
                  "settings": {
                    "image": {
                      "url": "https://via.placeholder.com/800x450/BAE6FD/99F6E4?text=Interactive+Map+Coming+Soon"
                    },
                    "image_size": "full",
                    "align": "center",
                    "border_radius": { "unit": "px", "size": 8 }
                  }
                }
              ]
            }
          ]
        }
      ]
    }
  ]
}

